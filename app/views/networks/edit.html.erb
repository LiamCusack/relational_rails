<%= form_tag "/networks/#{@network.id}", method: :patch do %>
  <%= label_tag :name, "Name:" %>
  <%= text_field_tag :name, @network.name %><br>
  <%= label_tag :established, "Established:" %>
  <%= text_field_tag :established, @network.established %><br>
  <%= label_tag :num_of_employees, "Number of Employees:" %>
  <%= text_field_tag :num_of_employees, @network.num_of_employees %><br>
  <%= submit_tag 'Update Network' %>
<% end %>