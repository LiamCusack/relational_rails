<%= form_tag "/networks", method: :post do %>
  <%= label_tag :name, "Name:" %>
  <%= text_field_tag :name%><br>
  <%= label_tag :established, "Established:" %>
  <%= text_field_tag :established%><br>
  <%= label_tag :num_of_shows, "Number of Shows:" %>
  <%= text_field_tag :num_of_shows%><br>
  <%= submit_tag 'Create Network' %>
<% end %>